{
  "name": "@sealsystems/request-service",
  "version": "3.1.0",
  "description": "Makes a request to a service, using consul for discovery and lookup",
  "author": {
    "name": "SEAL Systems AG",
    "email": "opensource@sealsystems.de",
    "url": "https://www.sealsystems.de"
  },
  "license": "MIT",
  "scripts": {
    "bot": "bot",
    "release": "semantic-release --branch ${CIRCLE_BRANCH}",
    "test": "npm run bot test-units",
    "coverage": "nyc --reporter=text --reporter=lcov mocha",
    "report": "npm run coverage && if [[ $OSTYPE == darwin* ]]; then open=open; else open=xdg-open; fi && $open coverage/lcov-report/index.html"
  },
  "main": "lib/requestService.js",
  "dependencies": {
    "retry": "0.10.1",
    "@sealsystems/connect-service": "1.0.14",
    "@sealsystems/consul": "3.5.12",
    "@sealsystems/log": "1.2.4"
  },
  "devDependencies": {
    "@sealsystems/eslint-config-es": "1.1.1",
    "assertthat": "1.0.0",
    "nyc": "12.0.2",
    "proxyquire": "2.0.0",
    "roboter": "0.16.0",
    "roboter-server": "0.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sealsystems/node-request-service.git"
  },
  "release": {
    "extends": "@sealsystems/semantic-release-config/node-module"
  },
  "publishConfig": {
    "access": "public"
  }
}
